{
  "version": "1.2.0",
  "nodeModulesDir": "auto",
  "tasks": {
    "setup": "deno install --allow-scripts=npm:@matrix-org/matrix-sdk-crypto-nodejs@0.4.0",
    "dev": "deno run --watch --allow-all --env-file main.ts",
    "start": "deno run --allow-all --env-file main.ts",
    "test": "deno test --allow-all",
    "test:unit": "deno test tests/unit --allow-all",
    "test:feature": "deno test tests/feature --allow-all",
    "test:integration": "deno test tests/integration --allow-all",
    "lint": "deno lint"
  },
  "imports": {
    "matrix-bot-sdk": "npm:matrix-bot-sdk@^0.8.0",
    "@std/assert": "jsr:@std/assert@1",
    "@std/path": "jsr:@std/path@1"
  }
}
